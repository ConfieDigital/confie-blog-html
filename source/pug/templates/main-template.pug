doctype html
html(lang='en')
    head
        block title
        meta(charset='utf-8')
        block description
        meta(name='viewport', content='width=device-width, initial-scale=1')
        include ../includes/favicon
        link(rel='preload' as='font' href='fonts/OpenSans-Light.woff' type='font/woff' crossorigin)
        link(rel='preload' as='font' href='fonts/OpenSans-Semibold.woff' type='font/woff' crossorigin)
        link(rel='preload' as='font' href='fonts/OpenSans-Bold.woff' type='font/woff' crossorigin)
        link(rel='stylesheet', href='css/main.css')
    body
        include ../includes/header
        main.cb-main-container.cb-container
            block content
        include ../includes/footer
        script(src='js/main.js')